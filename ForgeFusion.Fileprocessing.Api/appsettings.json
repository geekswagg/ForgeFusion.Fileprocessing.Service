{
  "Storage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ContainerName": "files",
    "QueueName": "file-uploads",
    "TableName": "fileProcessing",
    "AuditTableName": "fileAudit",
    "InFolder": "in",
    "OutFolder": "out",
    "ArchiveFolder": "archive",
    "MaxFileSize": 209715200,
    "AllowedContentTypes": [
      "application/pdf",
      "image/png",
      "image/jpeg",
      "image/jpg",
      "text/csv",
      "application/csv",
      "text/plain",
      "application/octet-stream",
      "image/gif",
      "image/bmp",
      "application/zip",
      "application/x-zip-compressed",
      "text/html",
      "application/json",
      "application/xml",
      "text/xml"
    ],
    "AllowedExtensions": [
      ".pdf",
      ".png",
      ".jpg",
      ".jpeg",
      ".csv",
      ".txt",
      ".gif",
      ".bmp",
      ".zip",
      ".html",
      ".htm",
      ".json",
      ".xml"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
