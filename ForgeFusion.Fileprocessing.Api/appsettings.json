{
  "Storage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ContainerName": "files",
    "QueueName": "file-uploads",
    "TableName": "fileProcessing",
    "AuditTableName": "fileAudit",
    "InFolder": "in",
    "OutFolder": "out",
    "ArchiveFolder": "archive",
    "AllowedContentTypes": [
      "application/pdf",
      "image/png",
      "image/jpeg"
    ],
    "AllowedExtensions": [
      ".pdf",
      ".png",
      ".jpg",
      ".jpeg"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
